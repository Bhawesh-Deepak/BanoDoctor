<div id="divCreateUserPartial">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <div class="pull-right marT2i marR20 c-red">* Please upload the detail in the same format of Template we have provided you !</div>
        <h4 class="modal-title">Upload Candidate File</h4>
      
    </div>
    <form asp-controller="UploadCandidateDetail" asp-action="UploadFile" enctype="multipart/form-data" method="post">
        <div class="modal-body">

            <div class="row">
                <div class="col-xs-3 form-group">
                    <label class="control-label"><em>* </em>Upload Candidate File</label>
                    <input name="inputFile" type="file" class="form-control" autocomplete="off" />
                </div>
                <div class="col-xs-3 form-group">
                    <label class="control-label"><em>* </em>Download Template</label>
                    <a href="~/CandidateDetails.xlsx">Download Template</a>
                </div>
            </div>

        </div>
        <div class="modal-footer">
            <input type="submit" class="btn btn-default" />
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
    </form>
</div>