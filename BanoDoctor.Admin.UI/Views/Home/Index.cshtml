                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col-xs-12">
            <fieldset class="fieldset">
                <legend>New Request Details</legend>
                <div class="pad5">
                    <div class="row">
                        <div class="box sH">
                            @*@await Html.PartialAsync(Views._dashBoardQuickLinkPartial)*@
                        </div>
                    </div>
