            <li><a asp-controller="UserDetail" asp-action="Index" class="usersIco" title="Users">Users</a></li>
            @*  <li><a asp-controller="ClaimTest" asp-action="Index" class="claimIco" title="Test Claim">Test Claim</a></li>*@
        </ul>
    </div>
</div>

