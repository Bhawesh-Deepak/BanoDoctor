            <li><a asp-controller="Formulary" asp-action="FormularyIndex" class="formularyIco" title="Formulary Data">Client</a></li>
            @*<li><a asp-controller="DrugSearch" asp-action="Index" class="drugIco" title="Drug Listing">Drugs</a></li>*@
