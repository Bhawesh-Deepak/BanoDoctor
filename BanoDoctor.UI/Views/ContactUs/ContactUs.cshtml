@model BanoDoctor.UI.CQRS.Commands.CreateInquiryCommand

    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>





<div class="hero-section">
<div class="container-fluid section-overlay text-center">

<div class="row">

<div class="col-sm-12 col-md-12 col-lg-12">
 <h2 class="color-white hero-title">Contact Us </h2>

</div>

</div>

</div>
</div>

<div class="section mt-5">
<div class="container pb-5 pt-5">

<div class="row">
<div class="col-md-8">
<div class="contact-box text-center shadow-lg p-5 rounded">

<h5 class="color-blue"><i class="fas fa-home me-3"></i>Address</h5>

<p>Office no 310 (Golden chember of comerce) Veera Desai Industrial Estate, Andheri West, Mumbai, Maharashtra 400102</p>
<hr>
<p>11, 5, Nath Mandir Rd, in front of Arora Bhawan, South Tukoganj, Indore, Madhya Pradesh 452001</p>
</div>
</div>

<div class="col-md-4">
<div class="contact-box text-center shadow-lg rounded p-5">

<h5 class="color-blue"> <i class="fas fa-envelope me-3"></i>Email</h5>

<p> info@banodoctor.com</p>

<h5 class="color-blue"><i class="fas fa-phone me-3"></i>Phone</h5>
<p>+91-7880109834</p>


</div>

</div>


</div>

</div>
</div>

<div class="section mt-5 mb-5">
<div class="container  pb-5">

<div class="row">
                <div class="col-sm-12 col-md-8 col-lg-8">
                    <form method="post" data-ajax="true" data-ajax-method="post"
                          asp-action="CreateInquiry" asp-controller="ContactUs" data-ajax-begin="AjaxBeginMethod()" data-ajax-success="Success">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input asp-for="CandidateName" class="form-control" placeholder="Enter Your Full Name">
                                    <span style="color:red" asp-validation-for="CandidateName"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input asp-for="CandidateEmail" class="form-control" placeholder="Enter Your Email ID">
                                    <span style="color:red" asp-validation-for="CandidateEmail"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input asp-for="CandidatePhone" class="form-control" placeholder="Enter Your Mobile Number">
                                    <span style="color:red" asp-validation-for="CandidatePhone"></span>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <input asp-for="SelectedCourse" class="form-control" placeholder="Select Course You are Interested">
                                    <span style="color:red" asp-validation-for="SelectedCourse"></span>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <div class="form-group">
                                    <textarea asp-for="Message" class="form-control" rows="5" placeholder="Write Your Message Here"></textarea>
                                    <span style="color:red" asp-validation-for="Message"></span>
                                </div>


                            </div>

                            <div class="action-btn">

                                <button id="btnSubmit" type="submit" class="btn custom-btn"> Submit </button>
                                <button id="btnSubmitLoading" style="display:none" class="btn custom-btn buttonload">
                                    <i class="fa fa-refresh fa-spin"></i>Loading...
                                </button>


                            </div>
                        </div>
                    </form>
                </div>
</div>



</div>



</div>
